<div id="reserves" class="reserves">
  <!--< if admin_user? %>-->
  <% if user_signed_in? && admin_user? %>
    <% logger.debug("================= admin_user") %>
    <%= render 'reserves/calender_staff_machine_admin' %>
  <% elsif user_signed_in? && !admin_user? %>
    <% logger.debug("================= user") %>
    <%= render 'reserves/calender_staff_machine' %>
  <% else %>
    <!--<= render 'reserves/system_mente' %>-->
    <% logger.debug("================= not_user") %>
    <%= render 'reserves/calender_staff_machine_not_user' %>
  <% end %>
</div>

